<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tjssm.mamsee.manager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sherlock" >
        <activity
            android:name="tjssm.mamsee.manager.ui.MainActivity">
        </activity>
	    <activity 
	    	android:label="@string/app_name" 
	    	android:windowSoftInputMode="adjustResize|stateVisible"
	    	android:name=".ui.LoginActivity"> 
	    </activity>
	    
	    <activity 
	        android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
	    	android:name=".ui.IntroActivity">
	    		<intent-filter>
	    			<action android:name="android.intent.action.MAIN"/>
	    			<category android:name="android.intent.category.LAUNCHER"/>
	    		</intent-filter>
	   	</activity>
	   	<activity 
	   	    android:name=".ui.CustomDialogActivity"
	   	    android:theme="@style/Theme.CustomDialog">
	   	</activity>
	   	<activity 
	   	    android:name=".ui.setting.DeleteDialog"
	   	    android:theme="@style/Theme.CustomDialog">
	   	</activity>
	   	<activity 
	   	    android:name=".ui.setting.InfoDialog"
	   	    android:theme="@style/Theme.CustomDialog">
	   	</activity>
	    <activity 
	   	    android:name=".mregister.MemberRegister">
	   	</activity>
    </application>
    
</manifest>
