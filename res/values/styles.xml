<resources>

    <style name="AppTheme" parent="android:Theme.Light" />

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="LoginFormContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">16dp</item>
    </style>
    
    <style name="AppTheme" parent="android:Theme.NoTitleBar" />
	<style name="MenuTheme" parent="android:Theme.NoTitleBar" />
	<style name="Theme.CustomDialog" parent="android:style/Theme.Dialog">
	</style>
	
<style name="mamsee" parent="Theme.Sherlock.Light">
<item name="android:actionBarStyle">@style/mamsee.ActionBarStyle</item>
<item name="actionBarStyle">@style/mamsee.ActionBarStyle</item>
</style>

<style name="mamsee.ActionBarStyle" parent="Widget.Sherlock.Light.ActionBar">
<item name="android:titleTextStyle">@style/mamsee.ActionBar.TitleTextStyle</item>
<item name="titleTextStyle">@style/mamsee.ActionBar.TitleTextStyle</item>
</style>

<style name="mamsee.ActionBar.TitleTextStyle" parent="TextAppearance.Sherlock.Widget.ActionBar.Title">
<item name="android:textColor">@android:color/darker_gray</item>
</style>
	
	
	
</resources>